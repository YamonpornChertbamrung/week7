pipeline {
    agent any
    parameters {
        string(name: 'NAME', defaultValue: 'Yamonporn', description: 'Your Name?')
        string(name: 'Ages', defaultValue: '21' , description: 'Your Ages?')
    }
    stages {
        stage('Example') {
            steps {
               echo "Name : ${params.NAME}  Age : ${params.Ages}"
                
            }
        }
    }
}
